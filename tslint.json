{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      140
    ],
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
