{
  "name": "api",
  "version": "0.0.1",
  "description": "api",
  "main": "dist/app/app.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p src/tsconfig.app.json && tsc -p orm/tsconfig.orm.json",
    "sync": "node dist/app/models/sync.js",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "start": "node dist/app/app.js",
    "predevelop": "npm run clean && npm run build && npm run sync && npm run seed",
    "develop": "concurrently --kill-others \"tsc -w -p src/tsconfig.app.json\" \"cd ./dist/app && nodemon app.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "rm -rf dist/spec && tsc -p src/tsconfig.spec.json && jasmine --config=src/jasmine.spec.json",
    "e2etest": "rm -rf dist/e2e && tsc -p e2e/tsconfig.e2e.json && jasmine --config=e2e/jasmine.e2e.json"
  },
  "author": "Andrey Korovin",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.5.53",
    "@types/jsonwebtoken": "^7.2.1",
    "@types/pg": "^6.1.41",
    "@types/request": "0.0.45",
    "@types/sequelize": "^4.0.62",
    "concurrently": "^3.5.0",
    "jasmine": "^2.6.0",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "sequelize-cli": "^2.7.0",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  }
}
